+  entityType   String
+  entityId     String?
+  metadata     Json?
+  teamId       String?
+  team         Team?        @relation(fields: [teamId], references: [id])
+
+  @@index([teamId])
+  @@index([userId])
+}
+
+model IdentityProvider {
+  id           Int          @id @default(autoincrement())
+  createdAt    DateTime     @default(now())
+  updatedAt    DateTime     @updatedAt
+  name         String
+  type         IdPType
+  config       Json
+  team         Team         @relation(fields: [teamId], references: [id])
+  teamId       String
+  enabled      Boolean      @default(true)
+
+  @@index([teamId])
+}
